
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3">Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @jabs.each do |jab| %>
    <div>
        <% if jab.user %>
   <%= link_to jab.user.username, user_path(jab.user) %>
   <% end %>
   </div>
      <tr>
        <td><%= jab.title %></td>
        <td><%= jab.description %></td>
        <td><%= link_to 'View', jab_path(jab) %></td>
        <% if logged_in? && (jab.user) == current_user %>
        <td><%= link_to 'Edit', jab_path(jab) %></td>
        <td><%= link_to 'Delete', jab_path(jab), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
      <% end %>
    <% end %>
  </tbody>
</table> 
<p>
    <%= link_to 'Throw new jab', new_jab_path %>
</p>